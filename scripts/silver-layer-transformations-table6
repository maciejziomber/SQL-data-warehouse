-- Table 6 transformations from bronze to silver layer
-- Query:
SELECT
id,
cat,
subcat,
maintenance
FROM bronze.erp_px_cat_g1v2

-- Check for unwanted spaces for all columns
SELECT *
FROM bronze.erp_px_cat_g1v2
WHERE cat != TRIM(cat)

-- Data standardization and consistency (check all columns)

SELECT DISTINCT
maintenance
FROM bronze.erp_px_cat_g1v2

-- Insert into silver layer table
INSERT INTO silver.erp_px_cat_g1v2 (
    id,
    cat,
    subcat,
    maintenance
)
SELECT
id,
cat,
subcat,
maintenance
FROM bronze.erp_px_cat_g1v2

SELECT *
FROM silver.erp_px_cat_g1v2
